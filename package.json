  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev-old": "webpack-dashboard -- node server.js",
    "start": "node server.js",
    "server": "nodemon ./server/index.js",
    "dev": "run-p development-build-server development-build-client development-prepare-server-build development-server-delayed",
    "development-prepare-server-build": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "development-build-client": "babel-node ./webpack/webpack-dev-server-entry.js",
    "development-build-server": "better-npm-run development-build-server",
    "development-server": "better-npm-run development-server",
    "development-server-delayed": "run-s delay development-server",
    "delay": "node ./app/core/sleep 20000"
  },
  "betterScripts": {
    "development-build-server": {
      "env": {
        "NODE_PATH": "./app"
      },
      "command": "webpack --config ./webpack/server/dev.js --watch --colors --display-error-details"
    },
    "development-server": {
      "env": {
        "NODE_PATH": "./app"
      },
      "command": "nodemon ./app/core/server/start.js --watch ./build/server"
    }
  },
